set terminal postscript eps enhanced  size 3.6in,1.2in
set output 'rich.eps'
set multiplot layout 1,2
set nokey

#set xlabel "Degree"
#set ylabel "Count"

set xrange [1:1000000]
set yrange [1:100000000]
set xtics (1,100,10000,1000000)
set ytics (1,100,10000,1000000,100000000)
set format xy "10^%L"
set logscale xy
FILES = system("ls -1 temp_ttttttttt29_out/part-*")
plot for [data in FILES] data using 1:2 lt -1 pi -6 pt 7 ps 0.5

unset xrange
unset yrange
set xtics 5
set ytics 400000
unset format x
unset logscale xy
set format y "%.0f"
set xrange [0:35]
FILES = system("ls -1 temp_ttttttttt29_in/part-*")
plot for [data in FILES] data using 1:2  lt -1 pi -6 pt 7 ps 0.5

unset multiplot
